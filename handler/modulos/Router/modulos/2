#!../usr/bin/bash
R='\033[1;31m'                                                        G='\033[1;32m'                                                        Y='\033[1;33m'                                                        B='\033[1;34m'                                                        M='\033[1;35m'                                                        C='\033[1;36m'                                                        W='\033[0m'

echo $(clear)
echo -e "$C         _     _       _  $G           ____"
echo -e "$C        | |   (_)_ __ | | _____   $G  / ___|  ___ __ _ _ __ ___"
echo -e "$C        | |   | | '_ \| |/ / __|____\___ \ /$G __/ _  |  _   _ \\"
echo -e "$C        | |___| | | | |   <$G\__ \_____|__) | (_| (_| | | | | | |"
echo -e "$C        |_____|_|_| |_|_|\_\___/   $G |____/ \___\__,_|_| |_| |_|"
echo -e "$R                Created by @CesarHackGray && Mickey"
echo
